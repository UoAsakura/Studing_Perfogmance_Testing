              <stringProp name="script">// Списки популярных имен и фамилий.
def firstNames = [&quot;James&quot;, &quot;John&quot;, &quot;Robert&quot;, &quot;Michael&quot;, &quot;William&quot;, &quot;David&quot;, &quot;Richard&quot;, &quot;Joseph&quot;, &quot;Thomas&quot;, &quot;Charles&quot;]
def lastNames = [&quot;Smith&quot;, &quot;Johnson&quot;, &quot;Williams&quot;, &quot;Jones&quot;, &quot;Brown&quot;, &quot;Davis&quot;, &quot;Miller&quot;, &quot;Wilson&quot;, &quot;Moore&quot;, &quot;Taylor&quot;]
// Список шаблонов улиц с номерами домов.
def streets = [&quot;Main St&quot;, &quot;High St&quot;, &quot;Maple Ave&quot;, &quot;Broadway&quot;, &quot;Oak St&quot;, &quot;Pine St&quot;, &quot;Cedar St&quot;, &quot;Elm St&quot;, &quot;Washington Blvd&quot;, &quot;Lake Dr&quot;]
// Список городов и стран.
def cities = [
    &quot;New York, USA&quot;, &quot;Los Angeles, USA&quot;, &quot;Chicago, USA&quot;, &quot;Houston, USA&quot;, &quot;Miami, USA&quot;,
    &quot;London, UK&quot;, &quot;Manchester, UK&quot;, &quot;Birmingham, UK&quot;, &quot;Liverpool, UK&quot;, &quot;Edinburgh, UK&quot;,
    &quot;Berlin, Germany&quot;, &quot;Munich, Germany&quot;, &quot;Frankfurt, Germany&quot;, &quot;Hamburg, Germany&quot;, &quot;Cologne, Germany&quot;,
    &quot;Paris, France&quot;, &quot;Lyon, France&quot;, &quot;Marseille, France&quot;, &quot;Toulouse, France&quot;, &quot;Nice, France&quot;,
    &quot;Madrid, Spain&quot;, &quot;Barcelona, Spain&quot;, &quot;Valencia, Spain&quot;, &quot;Seville, Spain&quot;, &quot;Zaragoza, Spain&quot;
]

// Генерация случайного имени и фамилии.
String randomFirstName = firstNames[new Random().nextInt(firstNames.size())]
String randomLastName = lastNames[new Random().nextInt(lastNames.size())]
// Генерация случайного адреса улицы.
String randomStreetAddress = (1 + new Random().nextInt(999)).toString() + &quot; &quot; + streets[new Random().nextInt(streets.size())]
// Генерация случайного города и страны.
String randomCityAddress = cities[new Random().nextInt(cities.size())]
// Генерация случайного логина.
String randomLogin = randomFirstName + (1000 + new Random().nextInt(9000)) // Логин на основании имени.
// Генерация случайного пароля.
String randomPassword = UUID.randomUUID().toString().substring(0, 8) // 8-символьный пароль.

// Установка значений переменных в JMeter.
vars.put(&quot;Login&quot;, randomLogin)
vars.put(&quot;Password&quot;, randomPassword)
vars.put(&quot;FirstName&quot;, randomFirstName)
vars.put(&quot;LastName&quot;, randomLastName)
vars.put(&quot;StreetAddress&quot;, randomStreetAddress)
vars.put(&quot;CityAddress&quot;, randomCityAddress)
</stringProp>
