            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="CardNumberAndYear" enabled="true">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import java.time.Year
import java.util.Random

// Функция для генерации случайного номера карты в формате &apos;#### #### #### ####&apos;
def generateCardNumber() {
    Random random = new Random()
    def cardNumber = (1..4).collect { String.format(&quot;%04d&quot;, random.nextInt(10000)) }.join(&quot; &quot;)
    return cardNumber
}

// Функция для генерации случайного года в диапазоне от следующего года до следующего + 10 лет
def generateExpirationYear() {
    int currentYear = Year.now().getValue()
    int nextYear = currentYear + 1
    int maxYear = nextYear + 10
    Random random = new Random()
    return random.nextInt((maxYear - nextYear) + 1) + nextYear
}

// Генерация значений
def cardNumber = generateCardNumber()
def expirationYear = generateExpirationYear()

// Сохранение значений в переменные JMeter
vars.put(&quot;cardNumber&quot;, cardNumber)
vars.put(&quot;expirationYear&quot;, expirationYear.toString())

// Логирование (для отладки)
// log.info(&quot;Generated card number: &quot; + cardNumber)
// log.info(&quot;Generated expiration year: &quot; + expirationYear)</stringProp>
