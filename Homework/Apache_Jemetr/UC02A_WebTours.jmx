              <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Different_date_1.0" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">// Сегодня.
today = new Date()
// Генерация случайного числа от 1 до 10.
offset = new Random().nextInt(10)
// Инициализация двух дат в пределах 20 дней.
date1 = today.plus(offset).format(&quot;MM/dd/yyyy&quot;)
date2 = today.plus(offset * 2).format(&quot;MM/dd/yyyy&quot;)

vars.put(&quot;earlierDate&quot;, date1)
vars.put(&quot;laterDate&quot;, date2)</stringProp>
                <stringProp name="TestPlan.comments">Генерируем две даты</stringProp>
              </JSR223PreProcessor>
              <hashTree/>
              <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Different_date_1.1" enabled="false">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import java.time.LocalDate
import java.time.format.DateTimeFormatter
import java.util.Random

// Диапазон в днях для генерации случайных дат (например, 30 дней)
int daysRange = 30

// Получаем сегодняшнюю дату
LocalDate today = LocalDate.now()

// Генерируем две случайные даты в пределах 30 дней от сегодняшней даты
Random random = new Random()
LocalDate randomDate1 = today.plusDays(random.nextInt(daysRange + 1))
LocalDate randomDate2 = today.plusDays(random.nextInt(daysRange + 1))

// Форматируем даты в нужном формате MM/dd/yyyy
DateTimeFormatter formatter = DateTimeFormatter.ofPattern(&quot;MM/dd/yyyy&quot;)
String formattedDate1 = randomDate1.format(formatter)
String formattedDate2 = randomDate2.format(formatter)

// Определяем, какая из дат позже, и сохраняем их в переменные JMeter с именами, указывающими на результат сравнения
if (randomDate1.isAfter(randomDate2)) {
    vars.put(&quot;laterDate&quot;, formattedDate1)
    vars.put(&quot;earlierDate&quot;, formattedDate2)
    log.info(&quot;Later date: &quot; + formattedDate1 + &quot;, Earlier date: &quot; + formattedDate2)
} else if (randomDate2.isAfter(randomDate1)) {
    vars.put(&quot;laterDate&quot;, formattedDate2)
    vars.put(&quot;earlierDate&quot;, formattedDate1)
    log.info(&quot;Later date: &quot; + formattedDate2 + &quot;, Earlier date: &quot; + formattedDate1)
} else {
    // Если случайно сгенерированные даты оказались равны, то повторяем генерацию для одной из них
    randomDate2 = today.plusDays(random.nextInt(daysRange + 1))
    formattedDate2 = randomDate2.format(formatter)
    if (randomDate1.isAfter(randomDate2)) {
        vars.put(&quot;laterDate&quot;, formattedDate1)
        vars.put(&quot;earlierDate&quot;, formattedDate2)
    } else {
        vars.put(&quot;laterDate&quot;, formattedDate2)
        vars.put(&quot;earlierDate&quot;, formattedDate1)
    }
    log.info(&quot;Later date: &quot; + vars.get(&quot;laterDate&quot;) + &quot;, Earlier date: &quot; + vars.get(&quot;earlierDate&quot;))
}

</stringProp>
              </JSR223PreProcessor>
